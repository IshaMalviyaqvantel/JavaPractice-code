package javap;

public class for_nested {
public static void main(String[] args) {
		
		
		
		for(int i=1;i<=10;i++) {
			for(int j=1;j<=5;j++) {
				for(int k=1;k<=2;k++) {
		System.out.println("the value of i " +i+"-----------j  " +j+"-----------k  " +k);
		
		// TODO Auto-generated method stub
		}
			} 
	}
	}

}
